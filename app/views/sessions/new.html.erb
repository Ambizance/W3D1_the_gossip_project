<h1>Page de connexion</h1>


<body>
  <%= form_tag url_for(action: 'create'), method: "get" do %>

    <%= label_tag 'email' %>
    <%= text_field_tag 'email', @user.email %>
    <br /> <br />

    <%= label_tag 'password' %>
    <%= password_field_tag 'password', @user.password %>
    <br /> <br />

    <%= submit_tag "Connexion" %>

  <% end %>
</body>